class FireArm
{
    field Ammo A1;

    constructor FireArm new(int val)
    {
        let A1 = Ammo.new(val);
        return this;

    }


    
    method void trigger(int Ax,int Ay)
    {
        var int fire_or_not;

        let fire_or_not = A1.fire(Ax,Ay);
        

        return;
    }

    method void fire_update()
    {
        do A1.draw_update();

        return;
    }

    
}